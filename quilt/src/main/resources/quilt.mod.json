{
  "schema_version": 1,
  "mixin": [
    "vinery.mixins.json",
    "vinery-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "examplemod",
    "version": "${version}",
    "metadata": {
      "name": "Vinery",
      "description": "Hi. this is vinery",
      "contact": {
        "sources": "https://github.com/architectury/architectury-templates"
      },
      "contributors": {
        "Cristelknight" : "Developer"
      },
      "license": {
        "name": "Perfectly Awesome License v1.0",
        "id": "ARR",
        "url": "https://theperfectlyawesomelicense.com/",
        "description": "This license does things and stuff and says that you can do things and stuff too!"
      },
      "icon": "assets/vinery/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",

    "entrypoints": {
      "init": [
        "satisfyu.vinery.quilt.VineryQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.2"
      },
      {
        "id": "architectury",
        "version": ">=6.2.43"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  }
}