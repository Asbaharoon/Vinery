{
  "schemaVersion": 1,
  "id": "vinery",
  "version": "${version}",
  "name": "Vinery",
  "description": "Vinery contains nearly everything needed for not only creating wine but also for building up your own vinery.",
  "authors": [
    "satisfy_u, Cristelknight, BaumeisterJO(weird name but a nice guy)"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "Insert License Here",
  "icon": "assets/vinery/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "satisfyu.vinery.fabric.VineryFabric"
    ],
    "client": [
      "satisfyu.vinery.client.VineryClient"
    ],
    "rei_client": [
      "satisfyu.vinery.compat.rei.VineryReiClientPlugin"
    ],
    "vinery": [
      "satisfyu.vinery.registry.StorageTypes"
    ],
    "fabric-datagen": [
      "satisfyu.vinery.fabric.datagen.ModDataGeneration"
    ]
  },
  "mixins": [
    "vinery.mixins.json",
    "vinery-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=1.19.4",
    "architectury": ">=8.1.79"
  }
}